{
	"name": "markdown-link-expander",
	"displayName": "markdown-link-expander",
	"description": "Easily create pretty Markdown links using its HTML title.",
	"version": "0.1.2-again",
	"publisher": "skn0tt",
	"repository": "https://github.com/Skn0tt/markdown-link-expander",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:markdown-link-expander.expand"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "markdown-link-expander.expand",
				"title": "Expand URL to Markdown"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.18",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"cheerio": "^1.0.0-rc.3"
	}
}
